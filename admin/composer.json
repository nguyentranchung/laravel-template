{
    "name": "nguyentranchung/admin",
    "description": "Admin Package",
    "keywords": [
        "admin",
        "laravel",
        "admin"
    ],
    "homepage": "https://github.com/nguyentranchung/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Tran Chung",
            "email": "nguyentranchung52th@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.11",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src",
            "Admin\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Admin\\Facades\\Admin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
